<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<?xdevuiml version="1.0"?>
<?template NONE?>
<?import com.vaadin.flow.component.button.Button?>
<?import com.vaadin.flow.component.orderedlayout.HorizontalLayout?>
<?import com.vaadin.flow.component.orderedlayout.VerticalLayout?>
<VerticalLayout xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="MainLayout" x:route="home">
    <x:constraints zpos="0" width="100%" height="100%" />
    <HorizontalLayout x:name="horizontalLayout">
        <x:constraints width="100%" height="-1px" flex-grow="0.0" />
        <Button x:name="btnSaveFahrzeug" text="Fahrzeuge neu erstellen und in Microstream speichern">
            <x:constraints width="0px" height="0px" flex-grow="0.0" />
            <x:event>click.onComponentEvent</x:event>
        </Button>
        <Button x:name="btnDeleteFahrzeuge" text="Fahrzeuge in Microstream lÃ¶schen">
            <x:constraints width="0px" height="0px" flex-grow="0.0" />
            <x:event>click.onComponentEvent</x:event>
        </Button>
    </HorizontalLayout>
    <Button x:name="btnPrintStoredFahrzeuge" text="In Microstream gespeicherte Fahrzeuge ausgeben">
        <x:constraints width="0px" height="0px" flex-grow="0.0" />
        <x:event>click.onComponentEvent</x:event>
    </Button>
    <Button x:name="btnDontSaveFahrzeug" text="Fahrzeuge neu erstellen und ausgeben">
        <x:constraints width="0px" height="0px" flex-grow="0.0" />
        <x:event>click.onComponentEvent</x:event>
    </Button>
    <Button x:name="button" text="Microstream stoppen">
        <x:constraints width="0px" height="0px" flex-grow="0.0" />
        <x:event>click.onComponentEvent</x:event>
    </Button>
</VerticalLayout>
